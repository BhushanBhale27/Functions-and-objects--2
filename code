class Employee
{
    //Write the code here
    getInfo(salary,numberofhrs){
       this.salary=salary
       this.numberofhrs=numberofhrs
        
    }
    AddSal(){
        if(this.salary<500)
        this.salary=this.salary+10
    }
    AddWork(){
        if(this.numberofhrs>6)
        this.salary=this.salary+5
    }
    
}
//Do Not change the Below  Code



var t=new Employee();
t.getInfo(400,7);
t.AddSal();
t.AddWork();
console.log(t.salary);
var x=new Employee();
x.getInfo(600,8);
x.AddSal();
x.AddWork();
console.log(x.salary);
